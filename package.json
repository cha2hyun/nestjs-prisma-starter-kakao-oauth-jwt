{
  "name": "nestjs-prisma-starter-kakao-oauth-jwt",
  "version": "0.0.1",
  "description": "nestjs-prisma-starter-kakao-oauth-jwt",
  "author": "cha2hyun",
  "license": "MIT",
  "keywords": [
    "nestjs",
    "prisma",
    "prisma client",
    "typescript",
    "passport",
    "graphql",
    "kakao-oauth",
    "jwt"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/cha2hyun/nestjs-prisma-starter-kakao-oauth-jwt.git"
  },
  "bugs": {
    "url": "https://github.com/cha2hyun/nestjs-prisma-starter-kakao-oauth-jwt/issues"
  },
  "scripts": {
    "build": "nest build",
    "format": "concurrently \"npx prisma format\" \"prettier --write \"src/**/*.ts\" \"test/**/*.ts\"\"",
    "dev": "nest start --watch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migrate:dev": "prisma migrate dev --preview-feature",
    "migrate:dev:create": "prisma migrate dev --create-only --preview-feature",
    "migrate:reset": "prisma migrate reset --preview-feature",
    "migrate:deploy": "npx prisma migrate deploy --preview-feature",
    "migrate:status": "npx prisma migrate status --preview-feature",
    "migrate:resolve": "npx prisma migrate resolve --preview-feature",
    "prisma:studio": "npx prisma studio",
    "prisma:generate": "npx prisma generate",
    "prisma:generate:watch": "npx prisma generate --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "start:db": "npm run migrate:up && npm run prisma:generate",
    "postinstall": "npm run prisma:generate",
    "docker:migrate": "docker-compose -f docker-compose.migrate.yml up -d",
    "docker:db": "docker-compose -f docker-compose.db.yml up -d",
    "docker:seed": "docker exec -it nest-api npm run seed",
    "docker": "docker-compose up -d",
    "docker:build": "docker-compose build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/server": "^4.9.5",
    "@devoxa/prisma-relay-cursor-connection": "2.2.3",
    "@nestjs/apollo": "^12.0.10",
    "@nestjs/axios": "^3.0.1",
    "@nestjs/common": "10.1.0",
    "@nestjs/config": "3.0.0",
    "@nestjs/core": "10.1.0",
    "@nestjs/graphql": "^12.0.10",
    "@nestjs/jwt": "10.1.0",
    "@nestjs/passport": "10.0.0",
    "@nestjs/platform-express": "10.1.0",
    "@nestjs/swagger": "7.1.2",
    "@prisma/client": "^5.0.0",
    "axios": "^1.6.1",
    "bcrypt": "5.1.0",
    "class-transformer": "0.5.1",
    "class-validator": "0.13.2",
    "concurrently": "^8.2.2",
    "graphql": "^16.8.1",
    "graphql-scalars": "1.22.2",
    "graphql-subscriptions": "2.0.0",
    "nestjs-prisma": "0.22.0-dev.0",
    "passport": "0.6.0",
    "passport-jwt": "4.0.1",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.8.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.6.3",
    "@commitlint/config-conventional": "^17.6.3",
    "@nestjs/cli": "10.1.10",
    "@nestjs/schematics": "10.0.1",
    "@nestjs/testing": "10.1.0",
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.70",
    "@types/bcrypt": "5.0.0",
    "@types/chance": "1.1.3",
    "@types/express": "4.17.17",
    "@types/jest": "^29.5.3",
    "@types/node": "^20.3.1",
    "@types/supertest": "2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.59.11",
    "@typescript-eslint/parser": "^5.59.11",
    "chance": "1.1.11",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "husky": "^8.0.3",
    "jest": "29.6.1",
    "lint-staged": "^13.2.2",
    "prettier": "2.8.8",
    "prisma": "5.0.0",
    "prisma-dbml-generator": "0.11.0-dev.0",
    "supertest": "6.3.3",
    "ts-jest": "29.1.1",
    "ts-loader": "9.4.4",
    "ts-node": "10.9.1",
    "tsconfig-paths": "4.2.0",
    "typescript": "^5.1.6"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": [
      "eslint --max-warnings=0",
      "prettier -w"
    ],
    "src/**/*.{json,css,scss,md}": [
      "prettier -w"
    ]
  }
}
